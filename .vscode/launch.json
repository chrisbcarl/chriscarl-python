    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Python] - Current",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "[Python] - Current (!justMyCode)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
        },
        {
            "name": "[Tools] - dev",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/chriscarl/tools/dev.py",
            "console": "integratedTerminal",
            "args": [
                // "create", "core.lib.stdlib.importlib", "--force",
                // "run", "dir", "vars", "--module", "builtins", "--print-help",
                // "audit", "relpath",
                "audit", "tdd",
                //
                "--log-level", "DEBUG",
            ]
        },
    ]
